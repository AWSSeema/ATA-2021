@{
    ViewBag.Title = "AddDetails";
    Layout = "~/Areas/Admin/Views/Shared/_MainLayout.cshtml";
     
    TCAssociationTool.Entities.Enquiries objEnquiriy = ViewBag.objEnquiriy;
}
<script>
     
    $(document).ready(function () {
        $('#wEnquiries').addClass('active');
        $('#Others').addClass('active');
        $('.alert-danger').append('<i class="fa fa-ban"></i><button class="close" aria-hidden="true" data-dismiss="alert" type="button">×</button>');
        $('.alert-success').append('<i class="fa fa-check"></i><button class="close" aria-hidden="true" data-dismiss="alert" type="button">×</button>');  
    });

    
         

</script>

<!-- breadcumb -->
<div class="content-header">
    <h1>View Enquiry </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i> Home</a></li>
        <li><a href="@Url.Action("Index", "Enquiry")"><i class="fa fa-file-text-o"></i>Enquiries</a></li>
        <li class="active"><i class="fa fa-file-text-o"></i> View Enquiry</li>
    </ol>
</div>
<!--end breadcumb -->
<div class="content">
    <!-- Main content -->
    <div id="divmessage">
        @if (TempData["message"] != null)
        { @Html.Raw(TempData["message"].ToString())}

        @if (ViewBag.message != null)
        { @Html.Raw(ViewBag.message.ToString())}
    </div>
    <div class="clearfix sepH_b"></div> 
        <div class="box validation">
            <div class="box-header">
                <h3 class="box-title dis-in">Enquiry Details </h3>
            </div>
            <div class="box-body">
                <div class="row">
                    <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Name : </label>
                        @objEnquiriy.Name
                    </div>
                    <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Email : </label>
                        @objEnquiriy.Email
                    </div>
                    <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Mobile No : </label>
                        @objEnquiriy.PhoneNo
                    </div>    
                    <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Subject : </label>
                        @objEnquiriy.Subject
                    </div>             
                    <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Description : </label>
                        @objEnquiriy.Description
                    </div>
                     <div class="col-xs-12 col-lg-4 col-sm-6">
                        <label>Enquiry Type : </label>
                        @objEnquiriy.Field1
                    </div>
                </div>
            </div> 
        </div> 
        <div class="box">
            <!-- /.box-header -->
            <div class="box-body text-right"> 
                <a class="btn btn-info" href="@Url.Action("Index", "Enquiry")">Cancel</a>
            </div>
            <!-- /.box-body -->
        </div> 
  
</div>



