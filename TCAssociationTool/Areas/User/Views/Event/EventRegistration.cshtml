        if ($.trim($('#City').val()) == '') { $('#City').focus().addClass('redborder'); $('#spanCity').show().html('City Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spanCity').hide().html(''); $('#City').removeClass('redborder'); }

        
        var validval = 1;
        for (v = 1; v <= @lstEventRegistrationTypes.Count; v++) {
            if ($('#people'+v).val() == '' && validval == 1) 
            {
                validval = 1;
            }
            else {
                validval = 0;
            }
        }
        if (validval == 1 && id == 1) {
            alert("Please select atleast one Registration Category .");
            return false;
        }

        if (!RegValidate()) {
            return true;
        }
        if ($.trim($('#txtCaptcha').val()) == '') { $('#txtCaptcha').focus().addClass('redborder'); $('#spantxtCaptcha').show().html('Captcha Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spantxtCaptcha').hide().html(''); $('#txtCaptcha').removeClass('redborder'); }
        //if ($('#txtCaptcha').val() == '') { $('#txtCaptcha').focus().addClass("validate"); return false; } else { $('#txtCaptcha').focus().removeClass("validate"); }
        var str = Common.GetCaptcha();

        if ($('#txtCaptcha').val() != str) { $('#txtCaptcha').val(''); document.getElementsByName('txtCaptcha')[0].placeholder = 'You entered wrong captcha.'; $('#txtCaptcha').focus().addClass("validate"); return false; } else { $('#txtCaptcha').focus().removeClass("validate"); }

        return true;
    }
    function RegValidate() {
        for (j = 1; j <= @lstEventRegistrationTypes.Count; j++) {
            if ('@Memberdetails.MemberId' != "0") {
                if ($('#people'+j).val() > 4) { $('#people'+j).val(''); document.getElementsByName('['+parseInt(j)-1+'].Count')[0].placeholder = 'Max 4'; $('#people' + j).focus().addClass("validate"); return false; } else { $('#people' + j).removeClass("validate"); }
            }
        } 
        return true;
    }

    function ResetValidateClass() {
        $('#ItemName').removeClass("validate");
