    function Validate(id) {

        if ('@objEvents.EventCategory.ToLower()' == "cultural events") {
            if ($('#ItemName').val() == "") { $('#ItemName').focus().addClass("validate"); document.getElementsByName('ItemName')[0].placeholder = 'Required'; return false; } else { $('#ItemName').focus().removeClass("validate"); }
            if ($('#ItemCategory').val() == "") { $('#ItemCategory').focus().addClass("validate"); return false; } else { $('#ItemCategory').focus().removeClass("validate"); }
            if ($('#ItemDuration').val() == "") { $('#ItemDuration').focus().addClass("validate"); document.getElementsByName('ItemDuration')[0].placeholder = 'Required'; return false; } else { $('#ItemDuration').focus().removeClass("validate"); }
            if ($('#ItemDescription').val() == "") { $('#ItemDescription').focus().addClass("validate"); return false; } else { $('#ItemDescription').focus().removeClass("validate"); }
        }
        if ($('#DocumentUrl').val() == "") { $('#DocumentUrl').focus(); $('#Aspanfile').html('Required.'); return false; } else { $('#Aspanfile').html(''); }
        //if (!Common.isDocument($('#DocumentUrl').val())) { $('#Aspanfile').html('Sorry document is invalid. Only allowed extensions are:<b>doc, docx, txt, xls, xlsx, pdf</b>.'); $('#DocumentUrl').focus(); return false; } else { $('#Aspanfile').html(''); }
        if ($.trim($('#FirstName').val()) == '') { $('#FirstName').focus().addClass('redborder'); $('#spanFirstName').show().html('First Name Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spanFirstName').hide().html(''); $('#FirstName').removeClass('redborder'); }
        if ($.trim($('#LastName').val()) == '') { $('#LastName').focus().addClass('redborder'); $('#spanLastName').show().html('Last Name Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spanLastName').hide().html(''); $('#LastName').removeClass('redborder'); }
        if ($.trim($('#EmailId').val()) == '') { $('#EmailId').focus().addClass('redborder'); $('#spanEmail').show().html('Email Id Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spanEmail').hide().html(''); $('#EmailId').removeClass("redborder"); }
