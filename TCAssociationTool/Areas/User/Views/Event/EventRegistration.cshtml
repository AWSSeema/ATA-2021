        if ($.trim($('#Mobile').val()) == '') { $('#Mobile').focus().addClass('redborder'); $('#spanMobile').show().html('Mobile No Required <span>&nbsp;</span><i>&nbsp;</i><abbr onclick="closeValidation(this);">&nbsp;</abbr>'); return false; } else { $('#spanMobile').hide().html(''); $('#Mobile').removeClass('redborder'); }

        //if ($('#Mobile').val() == "") { $('#Mobile').focus().addClass("validate"); document.getElementsByName('Mobile')[0].placeholder = 'Required'; return false; } else { $('#Mobile').focus().removeClass("validate"); }
        //var mobile = $('#Mobile').val();
