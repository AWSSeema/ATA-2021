        $('#ItemDuration').removeClass("validate");
        $('#ItemDescription').removeClass("validate");
        $('#FirstName').removeClass("validate");
        $('#LastName').removeClass("validate");
        $('#EmailId').removeClass("validate");
        $('#Mobile').removeClass("validate");
        $('#City').removeClass("validate");
        $('#State').removeClass("validate");
        $('#txtCaptcha').removeClass("validate");

    }
    function CreloadCaptcha() {
        document.getElementById('CUcaptcha').src = document.getElementById('CUcaptcha').src + '/';
    }
    function calculate_total(plist) {
        document.getElementById("amount" + plist).innerHTML = "$" + ($('#hamount' + plist).val() * $('#people' + plist).val());
        @*for (j = 1; j <= @lstEventRegistrationTypes.Count; j++) {
            if ('@Memberdetails.MemberId' != "0") {
                    if ($('#people' + j).val() > 4) { $('#people' + j).val(''); document.getElementsByName('['+parseInt(j)-1+'].Count')[0].placeholder = 'Max 4'; $('#people' + j).focus().addClass("validate"); } else { $('#people' + j).removeClass("validate"); }
                }
                document.getElementById("amount" + j).innerHTML = "$" + ($('#hamount' + j).val() * $('#people' + j).val());
        }*@

        total5 = 0;
        var z = 0;
        for (i = 1; i <= @lstEventRegistrationTypes.Count; i++) {
            p = document.getElementById('people' + i).value;
            p = parseInt(p);
            if (p > 0) {
                total5 = total5 + (p * $('#hamount' + i).val());
            }
        }
        //total5 = parseFloat(total5 + (total5 * parseInt(3) / 100)).toFixed(2);
        document.getElementById("total").innerHTML = "$" + (total5);
        $('#AmountPaid').val(total5);        
        //document.getElementById("grassamount").innerHTML = "$" + (total5);
        if (total5 != 0) {
            $('#showamount').show();
            $('#paymentshow').show();
        }
        else {
            $('#paymentshow').hide();
        }

    }


    var specialKeys = new Array();
    specialKeys.push(8); //Backspace
    specialKeys.push(9); //Tab
    specialKeys.push(32); //Space
    specialKeys.push(46); //Delete
    specialKeys.push(36); //Home
    specialKeys.push(35); //End
    specialKeys.push(37); //Left
    specialKeys.push(39); //Right    


    function onlyAlphabets(e, t) {
        try {
            if (window.event) {
                var charCode = window.event.keyCode;
            }
            else if (e) {
                var charCode = e.which;
            }
            else { return true; }
            if ((charCode == 32) || (charCode > 64 && charCode < 91) || (charCode > 96 && charCode < 123) || (specialKeys.indexOf(e.keyCode) != -1 && e.charCode != e.keyCode))
                return true;
            else
                return false;
        }
        catch (err) {
            alert(err.Description);
        }
    }
    function phonenochange(event) {
        if (event.keyCode == 8 || event.keyCode == 46) {
        }
        else {
            var ph = $('#Mobile').val();
            if (ph.length == 3) { $('#Mobile').val(ph + '-'); }
            if (ph.length == 7) { $('#Mobile').val(ph + '-'); }
        }
    }

    function closeValidation(field) {
        $(field).parent().hide();
    }
    function closeValidationOnClick(field) {
        $(field).next('span').hide();
