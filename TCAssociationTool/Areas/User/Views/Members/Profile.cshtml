        $.ajax({
            url: '@Url.Action("Profile", "Members")',
            type: 'GET',
            cache: false,
            data: {  PageNo: pageno, SortColumn: sortcolumn, SortOrder: sortorder },
            success: function (result) {
                $('#volunteer').html(result);
                PagerClickBind();
            }
        });
    }

    function UpdateHoursSpent(i, VolunteerId) {
        var HoursSpent = parseInt($('#HoursSpent' + i).val());
        $.ajax({
            type: 'POST',
            url: '@Url.Action("UpdateVolunteerHours", "Volunteer")',
            data: { HoursSpent: HoursSpent, VolunteerId: VolunteerId },
            success: function (result) {
                var div = '';
                if (result.ok) {
                    $("#divmessage").html(result.data);
                    VolunteersBind(1);
                }
                else {
                    $("#divmessage").html(result.data);
                    $('#vfailedmsg').html(result.data).show();
                }
            }
        });
    }

