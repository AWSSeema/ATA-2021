    function UpdateHoursSpent(i, VolunteerId) {
        var HoursSpent = parseInt($('#HoursSpent' + i).val());
        $.ajax({
            type: 'POST',
            url: '@Url.Action("UpdateVolunteerHours", "Volunteer")',
