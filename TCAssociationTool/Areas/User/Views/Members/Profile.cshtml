            success: function (result) {
                var div = '';
                if (result.ok) {
                    $("#divmessage").html(result.data);
                    VolunteersBind(1);
                }
                else {
                    $("#divmessage").html(result.data);
                    $('#vfailedmsg').html(result.data).show();
                }
            }
        });
    }
