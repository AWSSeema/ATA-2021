        function CheckEmailAvailabilityforNL(email) {
            var returnval = null;
            $.ajax({
                type: "POST",
                url: '@Url.Action("CheckEmailAvailabilityforNL", "Home")', // the URL of the controller action method
                data: { Email: email }, // optional data
                datatype: "JSON",
                async: false,
                success: function (result) {
                    if (result.ok) {
                        returnval = result.data;
                    }
                }
            });
            return returnval;
        }
        function ValidateNewsLetter() {
            if ($('#NEmail').val() == "") { $('#NEmail').focus().addClass("validate"); return false; } else { $('#NEmail').removeClass("validate"); }
            if (!Common.CheckEmail($('#NEmail').val())) { $('#NEmail').val(''); $('#NEmail').attr('placeholder', 'Invalid email address'); $('#NEmail').focus().addClass("validate"); return false; } else { $('#NEmail').removeClass("validate"); }
            if (!CheckEmailAvailabilityforNL($.trim($('#NEmail').val()))) { $('#NEmail').val(''); $('#NEmail').attr('placeholder', 'Already has that email. Try another?.'); $('#NEmail').focus().addClass("validate"); return false; } else { $('#NEmail').removeClass("validate"); }
            return true;
        }

        function NResetValidateClass() {

            $('#NEmail').removeClass("validate");

        }

        function NewsLetterSubmit() {
            if (ValidateNewsLetter()) {
                var Email = $('#NEmail').val();
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("NewsLetterForm", "Home")', // the URL of the controller action method
                    data: { NEmail: Email }, // optional data
                    datatype: "JSON",
                    async: false,
                    success: function (result) {
                        if (result.ok) {
                            $('#formdiv').hide();
                            $('#messagediv').html(result.message).show();
                        }
                        else {
                            $('#messagediv').html(result.message).show();
                        }
                    }
                });
            }
        }

        function subscribenow() {
            $('#NEmail').val('');
            $('#NEmail').removeClass("validate");
            $('#SubscribeNow').dialog('open');
        }

        function EnqForm() {
