        $(document).ready(function () {
            if ('@ViewBag.str' == 'login-failed') {
                LoginShow();
                $('#ErrorMessage').show();
            }
            

            $('#login').dialog
             ({
                 autoOpen: false,
                 width: 600,
                 modal: true
             });

            $('#forgot-password').dialog
