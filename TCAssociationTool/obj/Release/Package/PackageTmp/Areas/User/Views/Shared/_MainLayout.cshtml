                autoOpen: false,
                width: 600,
                modal: true
            });

            if ('@str1' != "") {
                $('#failedmsg1').html('@str1');
                $('#failedmsg1').show();
                $('#login1').dialog('open');
                forgotshow();

                $('#Email2').val('');

                $('#Email2').removeClass('validate');

                if ('@str1' == 'Password details sent to Email Id registered.') {
                      setTimeout(LoginShow, 5000);
