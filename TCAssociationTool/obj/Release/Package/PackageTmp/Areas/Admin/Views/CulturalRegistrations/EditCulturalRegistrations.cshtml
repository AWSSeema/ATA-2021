@{
    ViewBag.Title = "AddMailTemplate";
    Layout = "~/Areas/Admin/Views/Shared/_MainLayout.cshtml";

    TCAssociationTool.Entities.CulturalRegistrations objCulturalRegistrations = ViewBag.objCulturalRegistrations;
    string imgurl = System.Configuration.ConfigurationManager.AppSettings["baseurl"] + "Content/documents/";
}

<script type="text/javascript" src="ckeditor/ckeditor.js"></script>
<script type="text/javascript" src="ckfinder/ckfinder.js"></script>

<script type="text/javascript">
    function Validate() {
        var return_val = true;

        if ($.trim($('#item_title').val()) == '') { document.getElementsByName('item_title')[0].placeholder = 'Required'; $('#item_title').focus().addClass("validate"); return false; } else { $('#item_title').focus().removeClass("validate"); }
        if ($.trim($('#item_type').val()) == '') { document.getElementsByName('item_type')[0].placeholder = 'Required'; $('#item_type').focus().addClass("validate"); return false; } else { $('#item_type').focus().removeClass("validate"); }


        return return_val;
    }



    function ResetValidateClass() {

        $('#item_title').removeClass("validate");
        $('#item_type').removeClass("validate");

    }

    $(document).ready(function () {
        $('#wGolf2021').addClass('active');
        $('#Others').addClass('active');
        $('.alert-danger').append('<i class="fa fa-ban"></i><button class="close" aria-hidden="true" data-dismiss="alert" type="button">×</button>');
        $('.alert-success').append('<i class="fa fa-check"></i><button class="close" aria-hidden="true" data-dismiss="alert" type="button">×</button>');
      //  $(".date").datepicker({ changeMonth: true, changeYear: true, yearRange: "-90:+0", altFormat: 'mm/dd/yy' });
        $("#datesent").datepicker({ changeMonth: true, changeYear: true, yearRange: "-50:+50", altFormat: 'dd/mm/yy', dateFormat: "dd/mm/yy" });


    });
</script>

<!-- breadcumb -->
<div class="content-header">
    <h1>Cultural Registrations</h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Index", "CulturalRegistrations")"><i class="fa fa-print"></i>Cultural Registrations</a></li>
        <li class="active">Edit Cultural Registration</li>
    </ol>
</div>
<!--end breadcumb -->


<div class="content">
    <!-- Main content -->
    <div id="divmessage">
        @if (TempData["message"] != null)
        {@Html.Raw(TempData["message"].ToString())}

        @if (ViewBag.message != null)
        {@Html.Raw(ViewBag.message.ToString())}
    </div>
    <div class="box validation">
        <!-- /.box-header -->
        <div class="box-header">
            <h3 class="box-title dis-in">Edit CulturalRegistrations </h3>
        </div>
        @using (Html.BeginForm("InsertCulturalRegistrations", "CulturalRegistrations", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input type="hidden" name="Id" id="Id" value="@objCulturalRegistrations.Id" />
            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">View Registration Details</h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Item Title</label>
                            <input type="text" class="form-control" name="item_title" value=" @objCulturalRegistrations.item_title" onclick="ResetValidateClass();" id="item_title">

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Item Type </label>
                            <input type="text" class="form-control" name="item_type" value=" @objCulturalRegistrations.item_type" onclick="ResetValidateClass();" id="item_type">

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label> Item Description </label>
                            <textarea id="item_description" name="item_description" class="form-control">@objCulturalRegistrations.item_description</textarea>

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Group Name</label>
                            <input type="text" class="form-control" name="group_name" value=" @objCulturalRegistrations.group_name" onclick="ResetValidateClass();" id="group_name">

                        </div>


                    </div>

                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Minutes </label>
                            <input type="text" class="form-control" name="minutes" value=" @objCulturalRegistrations.minutes" onclick="ResetValidateClass();" id="minutes">

                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Url</label>
                            <input type="text" class="form-control" name="url" value=" @objCulturalRegistrations.url" onclick="ResetValidateClass();" id="url">
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Choreographer</label>
                            <input type="text" class="form-control" name="choreographer" value=" @objCulturalRegistrations.choreographer" onclick="ResetValidateClass();" id="choreographer">
                        </div>


                    </div>


                </div>

            </div>



            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Contact Details</h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Address1</label>
                            <textarea id="address1" name="address1" class="form-control">@objCulturalRegistrations.address1</textarea>

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Address2 </label>
                            <textarea id="address2" name="address2" class="form-control">@objCulturalRegistrations.address2</textarea>

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label> City </label>
                            <input type="text" class="form-control" name="city" value=" @objCulturalRegistrations.city" onclick="ResetValidateClass();" id="city">

                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label> State </label>
                            <input type="text" class="form-control" name="state" value=" @objCulturalRegistrations.state" onclick="ResetValidateClass();" id="state">

                        </div>


                    </div>

                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>ZipCode </label>
                            <input type="text" class="form-control" name="zip" value=" @objCulturalRegistrations.zip" onclick="ResetValidateClass();" id="zip">

                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email</label>
                            <input type="text" class="form-control" name="email" value=" @objCulturalRegistrations.email" onclick="ResetValidateClass();" id="email">
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Mobile</label>
                            <input type="text" class="form-control" name="mobile" value=" @objCulturalRegistrations.mobile" onclick="ResetValidateClass();" id="mobile">
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name</label>
                            <input type="text" class="form-control" name="name" value=" @objCulturalRegistrations.name" onclick="ResetValidateClass();" id="name">
                        </div>
                      

                    </div>

                </div>

            </div>

            @*<div class="row">
                <div class="col-xs-12 col-lg-3 col-sm-6">
                    <label>Admin Comments </label>
                    <textarea id="comments" name="comments" class="form-control">@objCulturalRegistrations.comments</textarea>

                </div>

            </div>*@


            <div class="box">
                <!-- /.box-header -->
                <div class="box-body text-right">
                    <input type="submit" onclick="return Validate()" class="btn btn-primary" id="add" value="Submit" name="add">

                    <a class="btn btn-info" href="@Url.Action("Index", "CulturalRegistrations")">Cancel</a>
                </div>
                <!-- /.box-body -->
            </div>
        }
        <!-- /.box-body -->
    </div>
    <!-- /.content -->
</div>

