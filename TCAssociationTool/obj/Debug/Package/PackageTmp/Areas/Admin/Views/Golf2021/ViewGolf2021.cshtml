@{
    ViewBag.Title = "AddMailTemplate";
    Layout = "~/Areas/Admin/Views/Shared/_MainLayout.cshtml";

    TCAssociationTool.Entities.Golf2021 objGolf2021 = ViewBag.objGolf2021;
}


<script>

    function Validate() {
        var return_val = true;

        if ($.trim($('#comments').val()) == '') { document.getElementsByName('comments')[0].placeholder = 'Required'; $('#comments').focus().addClass("validate"); return false; } else { $('#comments').focus().removeClass("validate"); }


        return return_val;
    }
</script>
<script type="text/javascript" src="ckeditor/ckeditor.js"></script>
<script type="text/javascript" src="ckfinder/ckfinder.js"></script>
<!-- breadcumb -->
<div class="content-header">
    <h1>Golf2021</h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Index", "Golf2021")"><i class="fa fa-print"></i>Golf2021</a></li>
        <li class="active">View Golf2021</li>
    </ol>
</div>
<!--end breadcumb -->

<div class="content">
    <!-- Main content -->
    <div id="divmessage">
        @if (TempData["message"] != null)
        {@Html.Raw(TempData["message"].ToString())}

        @if (ViewBag.message != null)
        {@Html.Raw(ViewBag.message.ToString())}
    </div>
    <div class="box validation">
        <!-- /.box-header -->
        <div class="box-header">
            <h3 class="box-title dis-in">View Golf2021 </h3>
        </div>
        @using (Html.BeginForm("Golf2021CommentUpdate", "Golf2021", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input type="hidden" name="Id" id="Id" value="@objGolf2021.Id" />
            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Player1 Details </h3>
                </div>

                <div class="box-body">
                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Player Type </label>
                            @objGolf2021.playertype
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Amount </label>
                            @objGolf2021.amount
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Ip Address </label>
                            @objGolf2021.ip_address
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name </label>
                            @objGolf2021.player1_name
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Phone </label>
                            @objGolf2021.player1_phone
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email</label>
                            @objGolf2021.player1_email
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>T Shirt </label>
                            @objGolf2021.player1_tshirt
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Sponsorship </label>
                            @objGolf2021.sponsorship
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Payment Status </label>
                            @objGolf2021.PaymentStauts
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Payment Method</label>
                            @objGolf2021.payment_method
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Card No </label>
                            @objGolf2021.cardno
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Payment Response </label>
                            @objGolf2021.payment_response
                        </div>
                    </div>

                </div>

            </div>



            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Player2 Details </h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name </label>
                            @objGolf2021.player2_name
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Phone </label>
                            @objGolf2021.player2_phone
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email</label>
                            @objGolf2021.player2_email
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>T Shirt </label>
                            @objGolf2021.player2_tshirt
                        </div>

                    </div>


                </div>

            </div>


            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Player3 Details </h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name </label>
                            @objGolf2021.player3_name
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Phone </label>
                            @objGolf2021.player3_phone
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email</label>
                            @objGolf2021.player3_email
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>T Shirt </label>
                            @objGolf2021.player3_tshirt
                        </div>

                    </div>


                </div>

            </div>
            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Player4 Details </h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name </label>
                            @objGolf2021.player4_name
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Phone </label>
                            @objGolf2021.player4_phone
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email</label>
                            @objGolf2021.player4_email
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>T Shirt </label>
                            @objGolf2021.player4_tshirt
                        </div>

                    </div>


                </div>

            </div>


            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Other Details </h3>
                </div>

                <div class="box-body">
                    <div class="row">

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>IP </label>
                            @objGolf2021.ip
                        </div>

                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Vcode </label>
                            @objGolf2021.vcode
                        </div>

                        <div class="col-xs-12 col-lg-4 col-sm-6">
                            <label>Comments </label>
                            <textarea id="comments" name="comments" class="form-control">@objGolf2021.comments</textarea>

                        </div>

                    </div>


                </div>

            </div>


            <div class="box">
                <!-- /.box-header -->
                <div class="box-body text-right">
                    <input type="submit" onclick="return Validate()" class="btn btn-primary" id="add" value="Submit" name="add">
                    <a class="btn btn-info" href="@Url.Action("Index", "Golf2021")">Cancel</a>
                </div>
                <!-- /.box-body -->
            </div>
        }
        <!-- /.box-body -->
    </div>
    <!-- /.content -->
</div>

