@{
    ViewBag.Title = "ViewCommittee";
    Layout = "~/Areas/Admin/Views/Shared/_MainLayout.cshtml";

    TCAssociationTool.Entities.Committees objCommittees = ViewBag.objDetails;
}

<!-- breadcumb -->
<div class="content-header">
    <h1>View Committee Members</h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index", "Home")"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="@Url.Action("Index", "Committees")"><i class="fa fa-print"></i>Committees</a></li>
        <li class="active">View Committee Members</li>
    </ol>
</div>
<!--end breadcumb -->
<div class="content">
    <div class="box">
        <!-- /.box-header -->
        <div class="nav-tabs-custom">
            <ul class="nav nav-tabs">
                <li class="active"><a href="@Url.Action("Index", "Committees")">Committee Members</a></li>
                <li><a href="@Url.Action("Index", "CommitteeMaster")">Committees</a></li>
            </ul>
            <div class="clearfix sepH_b"></div>

            <div id="divmessage">
                @if (TempData["message"] != null)
                {@Html.Raw(TempData["message"].ToString())}

                @if (ViewBag.message != null)
                {@Html.Raw(ViewBag.message.ToString())}
            </div>

            <div class="validation">
                <!-- /.box-header -->
                <div class="box-header">
                    <h3 class="box-title dis-in">Member Details</h3>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Name : </label>
                            @objCommittees.Name
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Phone No :</label>
                            @objCommittees.PhoneNo
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>State : </label>
                            @(objCommittees.State != null && objCommittees.State != "" ? objCommittees.State : "N/A")
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Address :</label>
                            @(objCommittees.Address != null && objCommittees.Address != "" ? objCommittees.Address : "N/A")
                        </div>
                    </div>
                </div>
                <div class="box-body">
                    <div class="row">
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Email : </label>
                            @objCommittees.Email
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>City :</label>
                            @(objCommittees.City != null && objCommittees.City != "" ? objCommittees.City : "N/A")
                        </div>
                        <div class="col-xs-12 col-lg-3 col-sm-6">
                            <label>Display Order :</label>
                            @objCommittees.DisplayOrder
                        </div>
                    </div>
                </div>
            </div>
            <div class='box-body'>
                <label>Member Description </label><br />
                @Html.Raw(objCommittees.Description)
            </div>
        </div>
    </div>

    <div class="box">
        <div class="box-body text-right">
            <a class="btn btn-info" href="@Url.Action("Index", "Committees")">Cancel</a>
        </div>
    </div>

</div>

       



