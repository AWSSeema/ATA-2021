            if ($('#NEmail').val() == "") { $('#NEmail').focus().addClass("validate"); $('#NEmail').attr('placeholder', 'Required'); return false; } else { $('#NEmail').removeClass("validate"); $('#NEmail').attr('placeholder', ''); }
